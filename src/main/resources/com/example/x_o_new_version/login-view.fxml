<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.x_o_new_version.LoginController">
    <Button text="Create Game" onAction="#createGame"/>
    <Button text="List Games" onAction="#listGames"/>
    <ListView fx:id="gameListView"/>
    <Button text="Join Game" onAction="#joinGame"/>
    <Label fx:id="statusLabel" text=""/>
</VBox>